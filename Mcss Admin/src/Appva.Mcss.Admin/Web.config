<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <configSections>
    <section name="mcss" type="Appva.Mcss.Admin.Configuration.ApplicationConfiguration, Appva.Mcss.Admin" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="siths" type="Appva.Siths.Configuration.SithsConfiguration, Appva.Siths" />
  </configSections>

  <!-- Application configuration -->
  <mcss environment="Development" />
  
  <!-- Siths configuration -->
  <siths serverAddress="http://localhost:8088" key="apiKey" secret="secret" redirectUrl="http://localhost:8082/auth/sign-in/external/siths/token/" resellerId="none" />
  
  <!-- Logging configuration -->
  <log4net>
    <logger name="NHibernate">
      <level value="ERROR" />
    </logger>
    <logger name="NHibernate.SQL">
      <level value="ERROR" />
    </logger>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="admin.log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="yyyyMMdd" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%d [%t] %-5p %c - %m%n" />
      </layout>
    </appender>
    <appender name="EventLogAppender" type="log4net.Appender.EventLogAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%d [%t] %-5p %c - %m%n" />
      </layout>
    </appender>
    <appender name="TraceLogAppender" type="log4net.Appender.TraceAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%d [%t] %-5p %c - %m%n" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="TraceLogAppender" />
    </root>
  </log4net>
  <appSettings>
    <add key="EmailCreateAccountSubject" value="MCSS Konto skapat" />
    <add key="EmailCreateAccountBody" value="Hej {0}! Ditt lösenord är {1}" />
    <add key="EmailCreateBackEndAccountSubject" value="MCSS Konto skapat" />
    <add key="EmailCreateBackEndAccountBody" value="Hej {0}! Ditt lösenord i iPad är {1}. Ditt användarnman för att logga in på MCSS web är {2} med lösenordet {3}" />
    <add key="EmailEditAccountSubject" value="MCSS Konto skapat" />
    <add key="EmailEditAccountBody" value="Hej {0}! Ditt lösenord är {1}" />
    <add key="EmailReminderSubject" value="MCSS Påminnelse {0}" />
    <add key="EmailReminderBody" value="Detta är en påminnelse för {0}." />
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="owin:appStartup" value="OwinConfiguration" />
  </appSettings>
  <system.web>
    <globalization enableClientBasedCulture="true" uiCulture="sv-SE" culture="sv-SE" />
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <modules>
      <remove name="FormsAuthentication" />
    </modules>
    <httpErrors errorMode="Custom" existingResponse="Replace">
      <remove statusCode="400" />
      <error statusCode="400" path="/Assets/error-pages/400.html" responseMode="ExecuteURL" />
      <remove statusCode="403" />
      <error statusCode="403" path="/Assets/error-pages/403.html" responseMode="ExecuteURL" />
      <remove statusCode="404" />
      <error statusCode="404" path="/Assets/error-pages/404.html" responseMode="ExecuteURL" />
      <!-- Conflict: conflict between certificate and 'subdomain' (tenant url) -->
      <remove statusCode="409" />
      <error statusCode="409" path="/Assets/error-pages/409.html" responseMode="ExecuteURL" />
      <!-- Precondition Failed: No certificate found -->
      <remove statusCode="412" />
      <error statusCode="412" path="/Assets/error-pages/412.html" responseMode="ExecuteURL" />
    </httpErrors>
  </system.webServer>
