@model Appva.Mcss.Admin.Models.UploadTenaObserverPeriodModel
@{
    SetTitle("Ladda upp mätvärden");
}

@if (Model.StatusCode == HttpStatusCode.Accepted)
{
    <div id="remote-response">
        <form action="@Url.Action("list","tena")" class="validate" http-response="@Model.StatusCode" response-title="Uppladdningen lyckades">
            <div class="width-limiter width-limiter--xs">
                <p>Uppladdat till TENA Identifi.</p>
                <p><strong>@Model.StatusMessage</strong></p> 

                <div class="form-controls form-controls--align-end form-controls--no-margin">
                    <a href="@Url.Action("list","tena")" class="button" data-dialog-close="true">Ok</a>
                </div>
            </div>
        </form>
    </div>
}
else
{
    <div id="remote-response">
        <form action="@Url.Action("list","tena")" class="validate" http-response="@Model.StatusCode" response-title="Uppladdningen misslyckades">
            <div class="width-limiter width-limiter--xs">
                @*<p>Uppladdning misslyckades.</p>*@
                <p><strong>@Model.StatusCode</strong>: @Model.StatusMessage</p>

                <div class="form-controls form-controls--align-end form-controls--no-margin">
                    <a href="@Url.Action("list","tena")" class="button" data-dialog-close="true">Ok</a>
                </div>
            </div>
        </form>
    </div>
}


