@model Appva.Mcss.Web.ViewModels.ScheduleViewModel
@{ ViewBag.Title = "Ny signeringslista"; }
<div class="std-panel std-form lb-panel-small lb-validate-form" data-valclass="ScheduleCreate" data-valparams="id=@Model.Id;url=@Url.Action("VerifyUnique", "Schedule")">
	@using (Html.BeginForm()) {
		<h3 class="section-title">Ny signeringslista</h3>
		<div class="select">
			@Html.LabelFor(m => m.ScheduleSetting)
			@Html.DropDownListFor(
				m => m.ScheduleSetting,
				new SelectList(Model.Items, "Value", "Text", Model.ScheduleSetting),
				"Välj typ ..."
			)
			@Html.ValidationMessageFor(m => m.ScheduleSetting)
		</div>
		<div class="submit">
			@Html.AntiForgeryToken()
			@Html.Submit("Spara")
			@Html.Cancel("Avbryt")
		</div>
	}
</div>