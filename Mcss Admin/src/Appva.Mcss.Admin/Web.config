    </bindings>
    <client>
      <endpoint address="http://localhost:82/Service.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ITenantService" contract="Wcf.ITenantService" name="BasicHttpBinding_ITenantService" />
    </client>
  </system.serviceModel>
