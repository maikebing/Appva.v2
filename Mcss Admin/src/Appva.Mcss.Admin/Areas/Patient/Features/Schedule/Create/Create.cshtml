@model CreateScheduleForm
@{ 
    SetTitle(Localizer["Ny_signeringslista"].ToString()); 
}
<div class="std-panel std-form lb-panel-small lb-validate-form" data-valclass="ScheduleCreate" data-valparams="id=@Model.Id;url=@Url.Action("VerifyUnique", "Schedule", new { Id = Model.Id })">
	@using (Html.Post())
    {
		<h3 class="section-title">@Localizer["Ny_signeringslista"]</h3>
		<div class="select">
			@Html.LabelFor(x => x.ScheduleSetting)
			@Html.DropDownListFor(
				x => x.ScheduleSetting,
				new SelectList(Model.Items, "Value", "Text", Model.ScheduleSetting),
				Localizer["Välj_typ_"].ToString()
			)
			@Html.ValidationMessageFor(x => x.ScheduleSetting)
		</div>
		<div class="submit">
			@Html.Submit(Localizer["Spara"].ToString())
			@Html.Cancel(Localizer["Avbryt"].ToString())
		</div>
	}
</div>